<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://petriflow.com/petriflow.schema.xsd">
    <id>entity</id>
    <initials>ENT</initials>
    <title>Entita</title>
    <icon>device_hub</icon>
    <defaultRole>true</defaultRole>
    <anonymousRole>true</anonymousRole>
    <transitionRole>false</transitionRole>
    <caseName>Entita</caseName>
    <data type="text">
        <id>entity_name</id>
        <title>Entita</title>
    </data>
    <transition>
        <id>t1</id>
        <x>300</x>
        <y>180</y>
        <label>Generic transition</label>
        <assignPolicy>auto</assignPolicy>

        <event type="assign">
            <id>t1_assign</id>
        </event>
        <event type="finish">
            <id>t1_finish</id>
        </event>
        <event type="cancel">
            <id>t1_cancel</id>
        </event>
        <event type="delegate">
            <id>t1_delegate</id>
        </event>
    </transition>
</document>